# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1
VITE_PUBLIC_API_BASE_URL=http://localhost:8000

# App Configuration
VITE_APP_NAME=Calendly Clone
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional scheduling platform

# Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_DATA=false

# External Services (OAuth Client IDs)
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_MICROSOFT_CLIENT_ID=your_microsoft_client_id
VITE_ZOOM_CLIENT_ID=your_zoom_client_id

# Notification Settings
VITE_ENABLE_PUSH_NOTIFICATIONS=false
VITE_ENABLE_REAL_TIME_UPDATES=false

# Cache Settings
VITE_CACHE_DURATION=300000
VITE_STALE_TIME=60000

# UI Settings
VITE_DEFAULT_THEME=light
VITE_ENABLE_ANIMATIONS=true
VITE_SIDEBAR_DEFAULT_COLLAPSED=false